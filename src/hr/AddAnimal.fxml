<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="728.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="hr.AddAnimalController">
   <children>
      <Label layoutX="172.0" layoutY="31.0" text="Add New Animal">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <Label layoutX="79.0" layoutY="132.0" text="Name (*)" />
      <Label layoutX="78.0" layoutY="188.0" text="Address (*)" />
      <Label layoutX="79.0" layoutY="255.0" text="Food_type  (*)" />
      <Label layoutX="79.0" layoutY="306.0" text="Vaccination  (*)" />
      <Label layoutX="74.0" layoutY="360.0" text="Last_Checkup_date" />
      <Label layoutX="74.0" layoutY="414.0" text="Characteristic  (*)" />
      <Label layoutX="74.0" layoutY="469.0" text="Description" />
      <TextField fx:id="addName" layoutX="155.0" layoutY="128.0" prefHeight="23.0" prefWidth="211.0" />
      <TextField fx:id="addAddress" layoutX="159.0" layoutY="184.0" prefHeight="23.0" prefWidth="211.0" />
      <TextField fx:id="addFood" layoutX="172.0" layoutY="251.0" prefHeight="23.0" prefWidth="197.0" promptText="ex) Meat, etc..." />
      <TextField fx:id="addVactin" layoutX="186.0" layoutY="302.0" prefHeight="23.0" prefWidth="179.0" promptText="O/X" />
      <TextField fx:id="addCharacter" layoutX="187.0" layoutY="410.0" prefHeight="23.0" prefWidth="179.0" promptText="ex) cat or dog..." />
      <TextField fx:id="addDescription" layoutX="161.0" layoutY="469.0" prefHeight="175.0" prefWidth="384.0" promptText="under 1000byte" />
      <DatePicker fx:id="addDate" layoutX="190.0" layoutY="356.0" prefHeight="23.0" prefWidth="179.0" />
      <Button fx:id="addAnimalButton" layoutX="329.0" layoutY="677.0" mnemonicParsing="false" onAction="#addAnimalButtonOnAction" text="Add">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="clearAnimalButton" layoutX="395.0" layoutY="677.0" mnemonicParsing="false" onAction="#clearAnimalButtonOnAction" text="Clear">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="closeAnimalButton" layoutX="473.0" layoutY="677.0" mnemonicParsing="false" onAction="#closeAnimalButtonOnAction" text="Close">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
