<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="530.0" prefWidth="561.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="hr.UpdateAnimalController">
   <children>
      <Label layoutX="40.0" layoutY="38.0" text="Animal Update">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Label layoutX="50.0" layoutY="144.0" text="Characteristic  (*)" />
      <Label layoutX="50.0" layoutY="199.0" text="Description" />
      <TextField fx:id="addCharacter" layoutX="163.0" layoutY="140.0" prefHeight="23.0" prefWidth="179.0" promptText="ex) cat or dog..." />
      <TextField fx:id="addDescription" layoutX="137.0" layoutY="199.0" prefHeight="175.0" prefWidth="384.0" promptText="under 1000byte" />
      <Button fx:id="updateAnimalButton" layoutX="151.0" layoutY="396.0" mnemonicParsing="false" onAction="#updateAnimalButtonOnAction" prefHeight="74.0" prefWidth="101.0" text="Update">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="clearAnimalButton" layoutX="297.0" layoutY="437.0" mnemonicParsing="false" onAction="#clearAnimalButtonOnAction" text="Clear">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="closeAnimalButton" layoutX="404.0" layoutY="437.0" mnemonicParsing="false" onAction="#closeAnimalButtonOnAction" text="Close">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
